        <value>var openApprovals = new GlideRecord("sysapproval_approver");&#13;
openApprovals.addQuery("sysapproval", current.sys_id);&#13;
openApprovals.addQuery("state", "requested");&#13;
openApprovals.query();&#13;
while (openApprovals.next()) {&#13;
	openApprovals.state = 'cancelled';&#13;
	openApprovals.comments = 'Parallel approval rejected.';&#13;
	openApprovals.update();&#13;
}</value>
